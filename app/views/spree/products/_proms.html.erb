
<div class="m-5">


<h1 class="h1 text-center text-muted">List of Promocodes</h1>
</div>
<% Spree::Promotion.active.each do |promotion| %>
<table class="table table-dark container">
  <thead>


  <tr>
    <th scope="col">id</th>
    <th scope="col">name</th>
    <th scope="col">advertise</th>
  </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= promotion.id%></td>
      <td><%= promotion.name %></td>
      <td><%= promotion.advertise %></td>
    </tr>
  </tbody>



<% end %>
</table>
